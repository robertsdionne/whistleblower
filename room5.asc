
// room script file

function StartAnimation() {
  ComputerHum.Stop();
  ManualShutdown.Play();
  mouse.Visible = false;
  if (knowledge) {
    Neutral.SetView(NEUTRALVIEW);
    Neutral.Animate(0, 10, eOnce, eBlock);
    Wait(2*35);
    Tinnitus.Play();
    Wait(9*35);
  } else {
    Wait(3*35);
    FadeOut(5);
  }
  QuitGame(false);
}

function Neutral_AnyClick() {
  QuitGame(false);
}