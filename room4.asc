function StartAnimation() {
  mouse.Visible = false;
  BreatheRun.Play(eAudioPriorityNormal, eRepeat);
  Ok.SetView(OKVIEW);
  Ok.Animate(0, 1, eOnce, eBlock);
  Ok.Animate(0, 1, eOnce, eBlock);
  Ok.Animate(0, 1, eOnce, eBlock);
  Ok.Animate(0, 1, eRepeat, eNoBlock);
}

function Ok_AnyClick() {
  QuitGame(false);
}
