// room script file

function StartAnimation() {
  ComputerHum.Stop();
  mouse.Visible = false;
  BreatheRun.Play(eAudioPriorityNormal, eRepeat);
  Ok.SetView(OKVIEW);
  Ok.Animate(0, 1, eRepeat, eNoBlock);
  Wait(15*35);
  mouse.UseModeGraphic(eModeWait);
  mouse.Visible = true;
}

function Ok_AnyClick() {
  RestartGame();
}
