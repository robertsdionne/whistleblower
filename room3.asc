function StartAnimation() {
  Terrible.SetView(TERRIBLEVIEW);
  Terrible.Animate(0, 1, eOnce, eBlock);
  Terrible.Animate(0, 1, eOnce, eBlock);
  Terrible.Animate(0, 1, eOnce, eBlock);
  Terrible.Animate(0, 1, eRepeat, eNoBlock);
}

function Terrible_AnyClick() {
  QuitGame(false);
}
